                $("#scroller1").simplyScroll({
                    autoMode: 'loop',
                    frameRate: '48',


                });


                $("#scroller2").simplyScroll({
                    autoMode: 'loop'
                });

            });
        })(jQuery);
    </script>
</head>

<body>
    <!--<div class="all">-->
    <div id="index_bg_up">

        <div id="contain">

           @Html.Partial("_Menu")

        </div>
    </div>


    <div class="buld">
        <ul id="scroller1">
            <li><img src="~/images/banner01.jpg"></li>
        </ul>
    </div>

    <div id="index_bg_down">

        <div id="contain">

            @Html.Partial("_Footer")

        </div>
    </div>





